<view class="column" class="mapPop">
   <view class="mapTitle">签到地点错误(请打开GPS进行签到)</view>
    <map
      id="myMap"
      style="width: 100%; height: 300px;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      polyline='{{polyline}}'
      circles='{{circles}}'
      include-points='{{includePoints}}'
      show-location = '{{true}}'
      scale = '{{ 16 }}'
    ></map>
  </view>